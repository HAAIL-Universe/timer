{
  "name": "timer-backend",
  "version": "1.0.0",
  "description": "RESTful backend API for Timer application",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "node --watch src/index.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "echo 'no linter configured'"
  },
  "keywords": ["timer", "rest", "api"],
  "license": "MIT",
  "dependencies": {
    "better-sqlite3": "^9.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.0",
    "morgan": "^1.10.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "jest": "^29.0.0",
    "supertest": "^6.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": ["**/__tests__/**/*.js", "**/*.test.js"],
    "setupFilesAfterSetup": ["./tests/setup.js"],
    "collectCoverageFrom": ["src/**/*.js"],
    "coverageDirectory": "coverage"
  }
}
